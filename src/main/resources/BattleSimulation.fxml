<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="599.0" prefWidth="874.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="no.ntnu.idatg2001.userinterface.controllers.BattleSimulationController">
   <children>
      <Button layoutX="293.0" layoutY="484.0" mnemonicParsing="false" onAction="#onStartSimulationButtonClick" prefHeight="38.0" prefWidth="311.0" text="Start simulation">
         <font>
            <Font size="16.0" />
         </font></Button>
      <ChoiceBox fx:id="terrainChoiceBox" layoutX="460.0" layoutY="436.0" prefHeight="26.0" prefWidth="114.0" />
      <Label layoutX="130.0" layoutY="73.0" text="Army 1:" />
      <Label layoutX="534.0" layoutY="73.0" prefHeight="18.0" prefWidth="53.0" text="Army 2:" />
      <TableView fx:id="army1TableView" layoutX="36.0" layoutY="113.0" prefHeight="200.0" prefWidth="379.0">
        <columns>
          <TableColumn fx:id="army1UnitTypeColumn" prefWidth="75.0" text="Unit Type" />
          <TableColumn fx:id="army1UnitNameColumn" prefWidth="75.0" text="Name" />
            <TableColumn fx:id="army1UnitHealthColumn" prefWidth="75.0" text="Health" />
